{
  "DeployDescriptor":
  [
    {
      "Property" : "ControlmServer",
      "Assign"   : "Sandbox"
    },
    {
      "Property" : "Application",
      "Replace"  : [ {"rja-flow-dev(.*)" : "rja-flow-prd$1"} ]
    },
    {
      "Property" : "SubApplication",
      "Replace"  : [ {"rja-jobs-dev(.*)" : "rja-jobs-prd$1"} ]
    },
    {
      "ApplyOn"  :  {"Type":"Folder"},
      "Property" : "@",
      "Replace"  : [ {"rja-gen-jobs(.*)" : "rja-gen-jobs-prd$1"} ]
    },
    {
      "ApplyOn"  :  {"Type":"Job"},
      "Property" : "@",
      "Replace"  : [ {"rja-dev-(.*)" : "rja-prd-$1"} ] 
    },
    {  
      "Property" : "SiteStandard",
      "Replace"  : [ {"SE Demo System Standard(.*)" : "SE Demo System Standard$1"} ]

    },
    {
      "Property": "Flow",
      "Source": "flow1",
      "Replace": [{ "rja-dev-(.*)": "rja-prd-$1"}
        ]
    }
  ]
}
