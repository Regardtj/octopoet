{

    "Defaults" : {
		"Application": "rja-flow",
		"SubApplication": "rja-jobs",
		"RunAs" : "ctmsand",
        "ControlmServer": "Sandbox",
        "Host": "zzz-linux-agents"
	},

  "rja-gen-jobs": {
    "Type": "Folder",
    "SiteStandard": "SE Demo System Standard",
    "BusinessFields": [
      {
        "Usercode": "rja"
      }
    ],
    
      "rja-echo-job-dev": {
      "Type": "Job:Command",
      "Command": "echo 'Hallo'"
      },

      "rja-show-dev": {
        "Type" : "Job:Command",
        "Command" : "ls -la"
    },
    
    "rja-wait-dev": {
        "Type" : "Job:Command",
        "Command" : "sleep 5"
    },
    
    "rja-print-dev": {
        "Type" : "Job:Command",
        "Command" : "pwd"
    },
    "rja-run-dev": {
          "Type" : "Job:Command",
          "Command" : "sleep 5"
    },
    "flow1": {
           "Type":"Flow",
           "Sequence":["rja-echo-job-dev", "rja-show-dev", "rja-wait-dev"]
    },
    "flow2": {
           "Type":"Flow",
           "Sequence":["rja-echo-job-dev", "rja-print-dev", "rja-wait-dev"] 	
    },
    "flow3": {
           "Type":"Flow",
           "Sequence":["rja-echo-job-dev", "rja-run-dev", "rja-wait-dev"]
        }
      }
    }
