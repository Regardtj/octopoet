{

    "Defaults" : {
		"Application": "rja-flow-dev",
		"SubApplication": "rja-jobs-dev",
		"RunAs" : "ctmsand",
        "ControlmServer": "Sandbox",
        "Host": "zzz-linux-agents"
	},

  "rja-gen-jobs": {
    "Type": "Folder",
    "SiteStandard": "SE Demo System Standard",
    "BusinessFields": [
      {
        "Usercode": "rja"
      }
    ],
    
      "rja-dev-echo": {
      "Type": "Job:Command",
      "Command": "echo 'Hallo'"
      },

      "rja-dev-show": {
        "Type" : "Job:Command",
        "Command" : "ls -la"
    },
    
    "rja-dev-wait": {
        "Type" : "Job:Command",
        "Command" : "sleep 5"
    },
    
    "rja-dev-print": {
        "Type" : "Job:Command",
        "Command" : "pwd"
    },
    "rja-dev-run": {
          "Type" : "Job:Command",
          "Command" : "sleep 5"
    },
    "flow1": {
           "Type":"Flow",
           "Sequence":["rja-dev-echo", "rja-dev-show", "rja-dev-wait"]
    },
    "flow2": {
           "Type":"Flow",
           "Sequence":["rja-dev-echo", "rja-dev-print", "rja-dev-wait"] 	
    },
    "flow3": {
           "Type":"Flow",
           "Sequence":["rja-dev-echo", "rja-dev-run", "rja-dev-wait"]
        }
      }
    }
