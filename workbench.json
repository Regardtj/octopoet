{
  "DeployDescriptor":
  [
    {
      "Property" : "ControlmServer",
      "Assign"   : "workbench"
    },
    {
      "Property" : "Application",
      "Replace"  : [ {"RJA_Development(.*)" : "RJA_Workbench$1"} ]
    },
    {
      "Property" : "SubApplication",
      "Replace"  : [ {"RJA_Dev_Flow(.*)" : "RJA_Work_Flow$1"} ]
    },
    {
      "ApplyOn"  :  {"Type":"Folder"},
      "Property" : "@",
      "Replace"  : [ {"RJA_DevelopmentFlow(.*)" : "RJA_WorkbenchFlow$1"} ]
    },
    {
      "ApplyOn"  :  {"Type":"Job"},
      "Property" : "@",
      "Replace"  : [ {"RJA_Dev_(.*)" : "RJA_Work_$1"} ] 
    },
    {  
      "Property" : "SiteStandard",
      "Replace"  : [ {"RJA_Dev_Standard(.*)" : "RJA_Work_Standard$1"} ]

    },
    {
      "Property" : "Event",
      "Replace"  : [ {"RJA_Dev_Job(.*)" : "RJA_Work_Job$1"} ]
      }
  ]
}
